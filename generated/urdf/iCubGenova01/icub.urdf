<robot name="test_two_links">
    <link name="base_link" />
    <link name="root_link">
        <inertial>
            <mass value="4.72" />
            <origin xyz="-10 -20 30" rpy="0 -0 0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="3.0" iyz="0.0" izz="5.0" />
        </inertial>
    </link>
    <link name="lap_belt_1">
        <inertial>
            <mass value="10.0" />
            <origin xyz="20 -10 -03" rpy="0 -0 0" />
            <inertia ixx="1.0004544" ixy="-4.263e-05" ixz="-3.889e-08" iyy="2.001141" iyz="0" izz="3.001236" />
        </inertial>
    </link>
   
    <joint name="torso_pitch" type="revolute">
        <origin xyz="0 0 0" rpy="-2.77556e-17 -1.5708 3.14159" />
        <axis xyz="0 1 6.12323e-17" />
        <parent link="root_link" />
        <child link="lap_belt_1" />
    </joint>
    
    <joint name="base_fixed_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 -0 0" />
        <axis xyz="0 0 0" />
        <parent link="base_link" />
        <child link="root_link" />
    </joint>
    
